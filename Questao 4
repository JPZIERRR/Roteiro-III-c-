using System;


public interface Ivoar
{
    void Voar();
}

public interface Inadar
{
    void Nadar();
}

public class Pato : Inadar, Ivoar
{
    public void Voar()
    {
        Console.WriteLine("Esse animal pode voar!");
    }

    public void Nadar()
    {
        Console.WriteLine("Esse animal pode nadar!");
    }
}

public class Aguia : Ivoar
{
    public void Voar()
    {
        Console.WriteLine("Esse animal pode voar!");
    }
}

public class Peixe : Inadar
{
    public void Nadar()
    {
        Console.WriteLine("Esse animal pode nadar!");
    }
}

public class Program
{
    static void Main()
    {
        Pato pato = new Pato();
        pato.Voar();
        pato.Nadar();

        Aguia aguia = new Aguia();
        aguia.Voar();

        Peixe peixe = new Peixe();
        peixe.Nadar();
    }
}
